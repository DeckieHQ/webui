<header>
  <nav class='navbar navbar-default'>
    <div class='container-fluid'>
      <div class='navbar-header'>
        {{#link-to 'index' class='navbar-brand'}}Deckie{{/link-to}}
      </div>
      <div>
        <ul class='nav navbar-nav navbar-right'>
          {{#if session.isAuthenticated}}
            <li>{{notifications-box class='notifications_count'}}</li>
            <li>{{#link-to 'events.new'}}{{t 'navigation.events'}}{{/link-to}}</li>
            <li>{{#link-to 'account.dashboard'}}{{t 'navigation.account'}}{{/link-to}}</li>
            <li><a href="#" {{action 'logout'}}>{{t 'navigation.logout'}}</a></li>
          {{else}}
            <li>{{#link-to 'registration'}}{{t 'navigation.registration'}}{{/link-to}}</li>
            <li>{{#link-to 'login'}}{{t 'navigation.login'}}{{/link-to}}</li>
          {{/if}}
          <li>{{#link-to 'feedbacks.new'}}{{t 'navigation.feedbacks'}}{{/link-to}}</li>
        </ul>
      </div>
    </div>
  </nav>
</header>
